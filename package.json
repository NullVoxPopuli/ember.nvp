{
  "name": "ember.nvp",
  "version": "0.1.0",
  "description": "ember project generator, alternative to the official blueprints, but more modern -- hopefully one day to upstream back in to ember-cli / official blueprints",
  "repository": {
    "type": "git",
    "url": "git@github.com:NullVoxPopuli/ember.nvp.git"
  },
  "type": "module",
  "bin": "./src/cli/index.js",
  "exports": "./src/index.js",
  "imports": {
    "#args": "./src/cli/args.js",
    "#bases/*": "./src/bases/*/index.js",
    "#layers": "./src/layers/index.js",
    "#utils/*": "./src/utils/*"
  },
  "scripts": {
    "lint": "pnpm lint:format",
    "format": "prettier . --write",
    "lint:format": "prettier . --check",
    "new": "node ./src/cli/index.js",
    "new:tmp": "./test/manual.sh",
    "test:node": "FORCE_COLOR=1 vitest run",
    "test:vitest": "FORCE_COLOR=1 vitest"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "babel-remove-types": "^1.1.0",
    "content-tag": "^4.1.0",
    "ember-apply": "^2.17.0",
    "execa": "^9.6.1",
    "latest-version": "^9.0.0",
    "package-name-regex": "^5.0.0"
  },
  "devDependencies": {
    "@ember/app-tsconfig": "^2.0.0",
    "@ember/library-tsconfig": "^2.0.0",
    "@glint/ember-tsc": "^1.0.9",
    "@glint/template": "^1.7.3",
    "@glint/tsserver-plugin": "^2.0.9",
    "ember-source": "^6.10.0",
    "prettier": "^3.7.4",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "release-plan": "^0.17.4",
    "vitest": "^4.0.17"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">= 24"
  }
}
